{
    "version": 4,
    "configurations": [
        {
            "name": "MinGW",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/MinGW/include"
            ],
            "compilerPath": "C:/MinGW/bin/gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86",
            "browse": {
                "path": [
                    "C:/MinGW/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
